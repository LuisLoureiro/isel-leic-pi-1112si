@using mvc.Models.Entities
@model CurricularUnit

@{
    ViewBag.Title = "Criar nova Unidade Curricular";
}

<h2>Criar Nova Unidade Curricular</h2>
@using (@Html.BeginForm())
{
    @Html.EditorForModel()
    
    <div class="well">
        <input type="submit" value="Guardar" class="btn primary"/>
        @Html.ActionLink("Cancelar", "Index", "Fuc", null, new { @class = "btn danger"})
    </div>
    
}

